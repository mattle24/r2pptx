#' @include slide.R
#' @include generics.R

setClass(
  "R2PptxChapterList",
  slots = c(
    name = "character",
    chapters = "list"
  ),
  prototype = list(
    chapters = list()
  )
)

R2PptxChapterList <- function(name = "") {
  new("R2PptxChapterList", name = name)
}


setClass(
  "R2PptxChapter",
  slots = c(
    name = "character",
    slides = "R2PptxSlideList"
  )
)

