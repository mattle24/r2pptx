# name --------------------------------------------------------------------

setGeneric("name", function(x) standardGeneric("name"))
setMethod("name", signature = signature("ANY"), function(x) x@name)

setGeneric("name<-", function(x, value) standardGeneric("name<-"))
setMethod("name<-", signature = signature("ANY"), function(x, value) {
  x@name <- value
  validObject(x)
  x
})


