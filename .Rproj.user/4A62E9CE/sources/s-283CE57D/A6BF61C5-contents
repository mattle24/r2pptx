#' @include generics.R
#' @include chapter.R
#' @include element.R
#' @include location.R
#' @include slide.R
#' @include presentation.R

setMethod(
  "+",
  signature(e1 = "ANY", e2 = "R2PptxChapter"),
  function(e1, e2) {
    addChapter(e1, e2)
  }
)
setMethod(
  "+",
  signature = signature(e1 = "ANY", e2 = "R2PptxSlide"),
  function(e1, e2) {
    addSlide(e1, e2)
  }
)
